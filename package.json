{
  "name": "gofood-xuatexcel",
  "version": "1.0.0",
  "description": "Ứng dụng xuất Excel dữ liệu phiếu nhập kho Gofood",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --inspect",
    "build": "electron-packager . GofoodExportExcel --platform=win32 --arch=x64 --out=release --overwrite",
    "build:builder": "node build.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "exceljs": "^4.4.0"
  },
  "build": {
    "appId": "com.gofood.xuatexcel",
    "productName": "Gofood Export Excel",
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "dir",
      "sign": false,
      "verifyUpdateCodeSignature": false
    },
    "electronDist": "./node_modules/electron/dist",
    "npmRebuild": false
  }
}
